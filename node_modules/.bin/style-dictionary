#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/style-dictionary@4.2.0_prettier@3.3.3/node_modules/style-dictionary/bin/node_modules:/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/style-dictionary@4.2.0_prettier@3.3.3/node_modules/style-dictionary/node_modules:/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/style-dictionary@4.2.0_prettier@3.3.3/node_modules:/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/style-dictionary@4.2.0_prettier@3.3.3/node_modules/style-dictionary/bin/node_modules:/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/style-dictionary@4.2.0_prettier@3.3.3/node_modules/style-dictionary/node_modules:/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/style-dictionary@4.2.0_prettier@3.3.3/node_modules:/Users/reginald.curtis/tangocard/design-tokens/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../style-dictionary/bin/style-dictionary.js" "$@"
else
  exec node  "$basedir/../style-dictionary/bin/style-dictionary.js" "$@"
fi
